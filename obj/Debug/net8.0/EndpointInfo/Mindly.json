    "/api/v{version}/Emocoes": {
      "post": {
        "tags": [
          "Emocoes"
        ],
        "summary": "Detecta risco e tópicos emocionais a partir de um texto.",
        "description": "Este endpoint envia o texto para o serviço externo (/detect) e retorna um indicador de alerta,\r\npalavras de alerta detectadas, tópicos identificados e uma pontuação de risco. Também marcamos\r\nalerta localmente se palavras explícitas de violência forem encontradas.",
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Texto a ser avaliado",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/EmocaoRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Retorna status de sucesso, alerta consolidado e o objeto de detecção"
          }
        }
      }
    },
